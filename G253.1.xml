<?xml version="1.0" encoding="utf-8"?>
<Metacode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="C:\Program Files (x86)\Tecnomatix\MachineConfigurator\resource\mc_metacode.xsd">
  <Name>G253.1</Name>
  <Variable>LV_matROT</Variable>
  <Variable>LV_matTemp</Variable>
  <Variable>LV_vecTrans</Variable>
  <Execute>
    <AssignCommand>
      <Variable>LV_matROT</Variable>
      <Value>createMatrix()</Value>
    </AssignCommand>
    <AssignCommand>
      <Variable>LV_matTemp</Variable>
      <Value>getTrafoMatrix("ROTATIONAL")</Value>
    </AssignCommand>
    <AssignCommand>
      <Variable>LV_vecTrans</Variable>
      <Value>getMatrixTranslation(LV_matTemp)</Value>
    </AssignCommand>
    <Command>
      <Name>RotateMatrixY</Name>
      <Param>LV_matROT</Param>
      <Param>getRadians(getJointValue("B"))</Param>
    </Command>
    <Command>
      <Name>RotateMatrixZ</Name>
      <Param>LV_matROT</Param>
      <Param>getRadians(getJointValue("C"))</Param>
    </Command>
    <Command>
      <Name>TranslateMatrix</Name>
      <Param>LV_matROT</Param>
      <Param>LV_vecTrans</Param>
    </Command>
    <Command>
      <Name>ActivateTransformation</Name>
      <Param>"ROTATIONAL"</Param>
      <Param>FALSE</Param>
      <Param>FALSE</Param>
    </Command>
    <Command>
      <Name>ResetTransformation</Name>
      <Param>"ROTATIONAL"</Param>
    </Command>
    <Command>
      <Name>SetTrafoMatrix</Name>
      <Param>"ROTATIONAL"</Param>
      <Param>LV_matROT</Param>
    </Command>
    <Command>
      <Name>ActivateTransformation</Name>
      <Param>"ROTATIONAL"</Param>
      <Param>TRUE</Param>
      <Param>TRUE</Param>
    </Command>
  </Execute>
</Metacode>