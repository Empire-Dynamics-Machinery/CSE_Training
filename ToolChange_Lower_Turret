##LANGUAGE AC
    INT nToolID;
    nToolID = getVariable("#4120");
    STRING strCarrierTool;
    STRING strCarrier;
    strCarrier = "TUR";
    
    move(AXIS, "TURRET", (nToolID-1)*30, 1.0);
    strCarrierTool = getCarrierTool(strCarrier, ITOS(nToolID));
    SetNextTool(strCarrierTool, strCarrier);
    ActivateNextTool(strCarrier);
##LANGUAGE NATIVE

